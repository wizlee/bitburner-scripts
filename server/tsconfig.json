{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": false,
    "strict": true,
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "*": [
        "../node_modules/*",
        "./*"
      ],
    },
    "types": [
      "node"
    ],
    "declaration": false,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
  },
  "include": [
    "./*"
  ],
}
